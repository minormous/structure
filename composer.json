{
    "name": "minormous/structure",
    "description": "Simple, immutable data structures",
    "keywords": [
        "data structure",
        "immutable",
        "hash map",
        "hash table",
        "dictionary",
        "ordered list",
        "unordered list",
        "list",
        "set"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Minormous Contributors",
            "homepage": "https://github.com/minormous"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "replace": {
        "shadowhand/destrukt": "self.version",
        "destruktphp/destrukt": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Minormous\\Structure\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minormous\\Structure\\": "tests"
        }
    }
}
